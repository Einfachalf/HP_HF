    "react-dom": "^18.2.0",
    "react-use": "^17.4.0",
    "replicate": "^0.21.0",
    "solid-js": "^1.2",
    "svelte": "^4.0.0",
    "vercel": "^32.3.0",
    "vue": ">=3.2.26"
